label_settings = SubResource("LabelSettings_gdxnu")
autowrap_mode = 1

[node name="text3" type="Label" parent="Page1"]
z_index = 1
layout_mode = 0
offset_left = 236.0
offset_top = 387.0
offset_right = 1736.0
offset_bottom = 436.0
text = "    可那些人物，事件，却仍然历历在目。"
label_settings = SubResource("LabelSettings_gdxnu")
autowrap_mode = 1

[node name="text4" type="Label" parent="Page1"]
z_index = 1
layout_mode = 0
offset_left = 236.0
offset_top = 467.0
offset_right = 1736.0
offset_bottom = 620.0
text = "    “我最终，会把这些都忘掉吗？”你喃喃自语。你不甘心如此，事情不该如此，于是，一种预感越来越强烈——"
label_settings = SubResource("LabelSettings_gdxnu")
autowrap_mode = 1

[node name="text5" type="Label" parent="Page1"]
z_index = 1
layout_mode = 0
offset_left = 236.0
offset_top = 597.0
offset_right = 1736.0
offset_bottom = 646.0
text = "    你一定要这个梦写于笔下，将一切的一切，写于笔下。"
label_settings = SubResource("LabelSettings_gdxnu")
autowrap_mode = 1

[node name="text6" type="Label" parent="Page1"]
